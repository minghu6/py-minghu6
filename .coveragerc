[run]
source = minghu6

[report]
omit = minghu6/email/*
       minghu6/gui/*
       minghu6/graphic/*
       minghu6/http/*
       minghu6/internet/*
       minghu6/security/*
       minghu6/tools/*
       
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
